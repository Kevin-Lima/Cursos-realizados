algoritmo "Cálculo do Delta"
var
   a,b,c,delta: real
inicio
      Escreva("Digite o valor de A: ")
      Leia(a)
      Escreva("Digite o valor de B: ")
      Leia(b)
      Escreva("Digite o valor de C: ")
      Leia(c)
      
      delta <-(b * b) - (4 * a * c)
      
      Escreval()
      Escreval("O valor de Delta é: ", delta:0:2)
fimalgoritmo
