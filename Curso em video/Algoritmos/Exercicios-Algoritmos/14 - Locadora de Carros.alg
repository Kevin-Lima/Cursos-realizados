algoritmo "Locadora de Carros"
var
   preco, km: real
   dias: inteiro
inicio
      Escreva("Qual a distância percorrida em km pelo carro? ")
      Leia(km)
      Escreva("O carro foi alugado por quantos dias? ")
      Leia(dias)

      preco <- (dias * 90) + (km * 0.20)

      Escreval("O preço total a se pagar será de R$", preco:0:2)
fimalgoritmo

